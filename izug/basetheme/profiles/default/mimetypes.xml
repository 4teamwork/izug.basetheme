<?xml version="1.0"?>
<object name="mimetypes_registry" meta_type="MimeTypes Registry">

 <!-- Common file formats -->
 <mimetype mimetypes="image/gif"
           icon_path="icon_dokument_bilddatei.gif" />

 <mimetype mimetypes="image/jpeg
                      image/pjpeg"
           icon_path="icon_dokument_bilddatei.gif" />

 <mimetype mimetypes="image/png"
           icon_path="icon_dokument_bilddatei.gif" />

 <mimetype mimetypes="image/bmp
                      image/x-bmp
                      image/x-MS-bmp"
           icon_path="icon_dokument_bilddatei.gif" />

 <mimetype mimetypes="message/rfc822"
           icon_path="icon_dokument_email.gif" />

 <mimetype mimetypes="text/plain"
           icon_path="icon_dokument_text.gif" />

 <mimetype mimetypes="text/html"
           icon_path="icon_dokument_webdokument.gif" />

 <mimetype name="Rich Text Format (RTF)"
           mimetypes="application/rtf"
           binary="True"
           extensions="rtf"
           globs="*.rtf"
           icon_path="icon_dokument_text.gif" />

 <mimetype mimetypes="application/zip
                      application/x-zip-compressed"
           icon_path="icon_dokument_zip.gif" />

 <mimetype mimetypes="application/pdf
                      application/x-pdf
                      image/pdf"
           icon_path="icon_dokument_pdf.gif" />

 <!-- Remove .ai from postscript types, we want it to be associated only with Adobe Illustrator -->
 <mimetype name="PS document"
           binary="True"
           extensions="ps eps"
           globs="*.ps"
           icon_path="ps.png"
           mimetypes="application/postscript" />

 <!-- Microsoft Office Formats -->

 <!-- MS Excel -->
 <mimetype name="Microsoft Excel Spreadsheet"
           binary="True"
           extensions="xls xlt xla xlb xlc xld xll xlm xlw"
           globs="*.xls *.xlt *.xla *.xlb *.xlc *.xld *.xll *.xlm *.xlw"
           icon_path="icon_dokument_excel.gif"
           mimetypes="application/vnd.ms-excel
                      application/msexcel
                      application/x-msexcel" />

 <mimetype name="Microsoft Excel Spreadsheet (OOXML)"
           binary="True"
           extensions="xlsx"
           globs="*.xlsx"
           icon_path="icon_dokument_excel.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />

 <mimetype name="Microsoft Excel Template (OOXML)"
           binary="True"
           extensions="xltx"
           globs="*.xltx"
           icon_path="icon_dokument_excel.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.spreadsheetml.template" />

 <!-- MS Word -->
 <mimetype name="Microsoft Word Document"
           binary="True"
           extensions="doc wiz dot"
           globs="*.doc *.wiz *.dot"
           icon_path="icon_dokument_word.gif"
           mimetypes="application/msword" />

 <mimetype name="Microsoft Word Document (OOXML)"
           binary="True"
           extensions="docx"
           globs="*.docx"
           icon_path="icon_dokument_word.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.wordprocessingml.document" />

 <mimetype name="Microsoft Word Template (OOXML)"
           binary="True"
           extensions="dotx"
           globs="*.dotx"
           icon_path="icon_dokument_word.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.wordprocessingml.template" />


 <!-- MS Powerpoint -->
 <mimetype name="Microsoft PowerPoint Presentation"
           binary="True"
           extensions="ppt pot pps ppa pwz ppz"
           globs="*.ppt *.pot *.pps *.ppa *.pwz *.ppz"
           icon_path="icon_dokument_powerpoint.gif"
           mimetypes="application/vnd.ms-powerpoint
                      application/powerpoint
                      application/mspowerpoint
                      application/x-mspowerpoint"/>

 <mimetype name="Microsoft PowerPoint Presentation (OOXML)"
           binary="True"
           extensions="pptx"
           globs="*.pptx"
           icon_path="icon_dokument_powerpoint.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.presentationml.presentation" />

 <mimetype name="Microsoft PowerPoint Template (OOXML)"
           binary="True"
           extensions="potx"
           globs="*.potx"
           icon_path="icon_dokument_powerpoint.gif"
           mimetypes="application/vnd.openxmlformats-officedocument.presentationml.template" />

 <!-- MS Visio -->
 <mimetype name="Microsoft Visio document"
           binary="True"
           extensions="vsd vss vst vsw vdx vsx vtx vsl"
           globs="*.vsd *.vss *.vst *.vsw *.vdx *.vsx *.vtx *.vsl"
           icon_path="icon_dokument_visio.gif"
           mimetypes="application/vnd.visio"/>

 <!-- MS Project -->
 <mimetype name="MS Project File"
           binary="True"
           extensions="mpp"
           globs="*.mpp"
           icon_path="icon_dokument_msproject.gif"
           mimetypes="application/vnd.ms-project"/>

 <mimetype name="MS Project File (generic)"
           binary="True"
           extensions="mpt mpv mpx"
           globs="*.mpt *.mpv *.mpx"
           icon_path="icon_dokument_msproject.gif"
           mimetypes="application/x-project"/>


 <!-- Adobe Formats -->

 <mimetype name="Adobe Indesign Document"
           binary="True"
           extensions="indd ind"
           globs="*.indd *.ind"
           icon_path="icon_dokument_indesign.gif"
           mimetypes="application/x-indesign"/>

 <mimetype name="Adobe Photoshop Document"
           binary="True"
           extensions="psd"
           globs="*.psd"
           icon_path="icon_dokument_photoshop.gif"
           mimetypes="image/vnd.adobe.photoshop"/>

 <mimetype name="Adobe Illustrator Document"
           binary="True"
           extensions="ai"
           globs="*.ai"
           icon_path="icon_dokument_illustrator.gif"
           mimetypes="application/illustrator"/>


 <!-- Various other file formats -->

 <mimetype name="Mindjet MindManager File"
           binary="True"
           extensions="mmap"
           globs="*.mmap"
           icon_path="icon_dokument_mindmanager.gif"
           mimetypes="application/vnd.mindjet.mindmanager"/>
</object>
