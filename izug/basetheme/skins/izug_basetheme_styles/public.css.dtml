/*
** Plone style sheet - Public-facing Elements
**
** Style sheet documentation can be found at http://plone.org/documentation
**
** You should preferrably use ploneCustom.css to add your own CSS classes and to
** customize your portal, as these are the base fundaments of Plone, and will
** change and be refined in newer versions. Keeping your changes in
** ploneCustom.css will make it easier to upgrade.
**
** Feel free to use whole or parts of this for your own designs, but give credit
** where credit is due.
**
*/

/* <dtml-with base_properties> (do not remove this :) */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either :) */

/* Accessibility elements, applied by JS */
body.largeText #visual-portal-wrapper { font-size: 120%; }
body.smallText #visual-portal-wrapper { font-size: 80%; }

/* Padding for the columns */

#portal-column-one .visualPadding {
    padding: 3.7em 0.5em 1em 0;
}
#portal-column-two .visualPadding {
    padding: 3.7em 0 1em 1em;
}

/* Compensate for the inline editing hover, since we have a 1px border */
body.kssActive h2.inlineEditable:hover,
body.kssActive h1.inlineEditable:hover,
body.kssActive h2.formlibInlineEditable:hover,
body.kssActive h1.formlibInlineEditable:hover {
    padding-bottom: 1px;
}


h3, h4, h5, h6 {
    border-bottom: none;
    font-weight: bold;
}

.documentFirstHeading {
    margin-top: 0;
    margin-bottom: 0 !important
}

.documentContent,
#content-core {
    font-size: 1.1em;
    padding: 0 0em 2em 0em !important;
    background: &dtml-backgroundColor;;
}
.documentContent #content-core {
    font-size: 1em;
    padding: 0 !important;
}
.documentContent ul {
    list-style-image: url(&dtml-portal_url;/icon_list.gif);
    list-style-type: square;
    line-height: 1.454em;
    margin: 0 0 1.091em 1.363em;
    color: &dtml-grey5;;
    text-indent: 0.182em;
}
.documentContent ol {
    line-height: 1.454em;
    margin: 0 0 1.091em 2.272em;
    color: &dtml-grey5;;
    text-indent: -0.454em;
}
.documentContent ul ul,
.documentContent ul ol,
.documentContent ol ul,
.documentContent ol ol {
    margin-left: 0;
    padding-left: 1.818em;
}
.documentContent ul li,
.documentContent ol li {
    margin-bottom: 0.363em;
}
.documentContent dl {
    margin: 0 0 1.091em 0;
    line-height: 1.455em;
    color: &dtml-grey5;;
}
.documentContent dl dt {
    margin: 0 0 0.363em 0;
}
.documentContent dl dd {
    margin: 0 0 0.363em 1.364em;
}
/* Links with differently colored link underlines - only for content */
.documentContent p a {
    text-decoration: underline;
}
.documentContent p a:visited {
    color: &dtml-linkVisitedColor;;
    background-color: transparent;
}
.documentContent p a:active {
    color: &dtml-linkActiveColor;;
    background-color: transparent;
}

#content a:target {
    background-color: #fd7 !important;
}
.documentContent li a {
    text-decoration: none;
}
.documentContent li a:visited {
    color: &dtml-linkVisitedColor;;
    text-decoration: underline;
}
.documentContent li a:active {
    color: &dtml-linkActiveColor;;
}

.documentContent dd a {
    text-decoration: none;
}
.documentContent dd a:visited {
    color: &dtml-linkVisitedColor;;
    background-color: transparent;
    text-decoration: underline;
}
.documentContent dd a:active {
    color: &dtml-linkActiveColor;;
    background-color: transparent;
}
.documentContent a:hover {
    color: #D52000;
}
/* End links */

/* End edit form elements */

#visual-portal-wrapper {
    margin: 0;
	padding: 0 0.9em;
	width: &dtml-portalMinWidth;;
}

/* Logo properties */
#portal-logo {
    display: block;
    height: 4.8em;
}
#portal-logo img {
    border: 0;
    padding: 0 0 0 2.4em;
}

/* The skin switcher at the top, only shows up if you have multiple skins available */
#portal-skinswitcher {
}
#portal-skinswitcher a {
    display: block;
    float: left;
}
#portal-top {
    /* Top section */
    margin: 2.4em 0 1.2em 0;
    padding: 0;
    background-color: &dtml-globalBackgroundColor;;
    padding: 0 0 3.2em;
}
#portal-header {
    background-color: white;
}
/* Site-wide action menu - font size, contact, index, sitemap etc */
#portal-siteactions {
    background-color: transparent;
    white-space: nowrap;
    list-style-image: none;
    list-style-type: none;
    margin: 1px 0 0 0;
    height: auto;
    line-height: 1em;
    padding: 1em 0 0.3em 0;
    border-top: 0.2em &dtml-borderStyle; &dtml-zugblue;;
    overflow: hidden;
    text-indent: 0;
}
#portal-siteactions li {
    display: inline;
    padding: 0 0 0 0.454em;
    font-size: 1.1em;
    background: transparent url(&dtml-portal_url;/action_separator.gif) no-repeat top left;
}
#portal-siteactions li:first-child {
    background: transparent none;
    padding-left: 0;
}
#portal-siteactions li a {
    height: auto;
    text-decoration: none;
    border-style: none;
    color: &dtml-grey4;;
}

#portal-siteactions li.selected a {
}
#portal-siteactions li a:hover {
    color: #D52000;
    text-decoration: underline;
}

/* Searchbox style and positioning */

#portal-searchbox {
    float: right;
    background-color: transparent;
    margin: 0.5em 0 0 0;
    padding:0 0.9em 0 0;
    text-align: right;
    text-transform: &dtml-textTransform;;
    white-space: nowrap;
    z-index: 2;
}
#portal-advanced-search {
    display: none;
}
#portal-advanced-search a {
    color: &dtml-discreetColor;;
    text-decoration: none;
    text-transform: &dtml-textTransform;;
}

/* LiveSearch styles */
.LSRes {
    font-family:  <dtml-var fontFamily>;
    visibility: visible;
    color: &dtml-globalFontColor;;
    background-color: &dtml-backgroundColor;;
    vertical-align: middle;
    display:block;
    list-style-image: none;
    list-style-type: none;
    text-align: left;
    min-width: 16.5em;
    text-transform: none;
    margin-left: 0;
    line-height: 1.1em;
}

#LSHighlight, .LSHighlight {
    background-color: &dtml-highlightColor;;
    color:&dtml-globalFontColor;;
}
.LSTable {
    margin: 1em 0;
}
.LSRow {
    white-space: normal;
    padding:0.5em;
    margin: 0;
    list-style-image: none;
    list-style-type: none;
    font-size:1.1em;
}
.LSRow a {
    text-decoration: none;
    font-weight:bold;
    white-space:nowrap
}
.LSRow .discreet {
    display: none;
}
.LSDescr {
    color: &dtml-discreetColor;;
    font-size: &dtml-fontSmallSize;;
    font-weight: normal;
    margin-left: 2.5em;
}
.LSResult {
    position: relative;
    display: block;
    text-align: right;
    padding-top: 0.4em;
    margin: 0;
    left: -5.5em;
    z-index: 3;
}
.LSShadow {
    position: relative;
    text-align: right;
}
.livesearchContainer {
    background-color: &dtml-backgroundColor;;
    margin-top: 0;
    padding: 0 !important;
    position: absolute;
    right: 0px;
    /* Currently exposes a rendering bug in Mozilla */
    top: 0;
    white-space: normal;
    font-family:  <dtml-var fontFamily>;
    visibility: visible;
    text-align: left;
    color: &dtml-globalFontColor;;
    border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
    width: 30em;
    text-transform: none;
}
* html .livesearchContainer {
    padding: &dtml-borderWidth; !important;
    padding-top: 0 !important;
    background-color: &dtml-globalBorderColor;;
    border: 0;
}
#livesearchLegend {
    line-height: 1.5em;
    margin-top: -1.8em;
    margin-left: -0.1em;
    border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
    border-bottom: 0;
    background-color: &dtml-backgroundColor;;
}
* html #livesearchLegend {
    margin-top: -1.9em;
    margin-left: -8px;
    position: relative;
}

/* Workaround for Internet Explorer's broken z-index implementation */
.LSIEFix {
    background-color: &dtml-backgroundColor;;
    padding: 0em !important;
    z-index: 20;
}
.LSBox {
    clear: left;
    float: left;
    text-align: right;
    padding-right: 1px;
    display:block;
}
#LSNothingFound {
    text-align: center;
    padding: 2px;
}
.LSBox label {
    font-weight: normal;
}

/* The global section tabs. */
#portal-globalnav {
    background-color: transparent;
    padding: 0em 0em 0em 2.5em;
    white-space: nowrap;
    list-style: none;
    margin: 0.8em 0 0 0;
    overflow: hidden;
    float: left;
}
#portal-globalnav li {
    display: block;
    float: left;
    border-color: &dtml-backgroundColor;;
    border-width: &dtml-borderWidth;;
    border-style: &dtml-borderStyle; &dtml-borderStyle; none &dtml-borderStyle;;
    height: 2.3em;
    margin-left: -0.1em;
    margin-bottom: 0;
    padding: 0;
    background-color: &dtml-inputBackgroundColor;;
}

#portal-globalnav li a {
    /* The normal, unselected tabs. They are all links */
    color: &dtml-backgroundColor;;
    text-decoration: none;
	display:block;
	font-size: 1.1em;
    padding: 0.3em 0.909em 0.4em;
}
#portal-globalnav li:hover {
    background-color: &dtml-red;;
}
#portal-globalnav li a:hover {
    text-decoration: underline;
    color: &dtml-backgroundColor; !important;
}
#portal-globalnav li.selected {
    background-color: white;
}
#portal-globalnav li.selected a {
    color: &dtml-grey4;;
}
#portal-personaltools-wrapper {
    width: 70%;
    float: right;
	text-align: right;
	margin: 0 0 0 0;
	height: 2.4em;
}

#portal-personaltools {
    /* Bar with personalized menu (user preferences, favorites etc) */
    margin: 0;
    list-style: none;
    display:inline;
    width: 100%;
    float: right;
    whitespace:nowrap;
    padding-top: 1.1em;
    overflow: hidden;
}
#portal-personaltools .portalUser {
    background: transparent url(&dtml-portal_url;/user.gif) center left no-repeat;
    padding-left: 18px;
}
#portal-personaltools .portalNotLoggedIn {
    /* Used on all descriptions relevant to those not logged in */
    padding: 0;
    background: transparent;
    background-image: none;
}
#portal-personaltools li {
    display: inline;
}
#portal-personaltools li a {
    color: &dtml-grey4;;
/*    border-right: 1px solid &dtml-grey4;;;*/
    background: url(&dtml-portal_url;/action_separator.gif) no-repeat scroll right top transparent;
    display:inline;
    font-size:1.1em;
    line-height:1em;
    margin-right:-1px;
    padding-left:0.2em;
    padding-right:0.4em;
    text-decoration:none;
}
#portal-personaltools li a:hover {
    text-decoration: underline;
    color: &dtml-red;;
}
#portal-personaltools li a#user-name {
    background-image: none;
    padding-right: 0;
}

#portal-personaltools .visualIconPadding {
    padding-left: 10px;
}
.visualCaseSensitive {
    text-transform: none;
}
#portal-languageselector {
    margin: 0 1em 0 1em;
    float:right;
}
#portal-languageselector li {
    display: inline;
}

#portal-breadcrumbs {
    line-height: 1.6em;
    margin-top: 1em;
    margin-bottom: 1em;
}
#portal-breadcrumbs a {
    text-decoration: none;
    color: &dtml-grey4;;
}
#portal-breadcrumbs a:hover {
    text-decoration: underline;
    color: &dtml-red;;
}
.breadcrumbSeparator {
    background: url(&dtml-portal_url;/breadcrumbs_separator.gif) no-repeat scroll 0 45% transparent;
    margin: 0 0.3em;
    margin: 0 0 0 0.3em;
    padding: 0 0.3em 0 0;
}
.addFavorite {
    vertical-align: bottom;
}
.documentEditable {
/*    padding: 0em 1em 1em 1em !important;
    margin: 0 -1em;*/
}
#content-news h1 {
    margin-bottom: 1em;
}

/* Only h5/h6 headlines in the content area should have the discreet color */
#content h5,
#content h6 {
    color: &dtml-discreetColor;;
}
.newsItem {
    margin-bottom: 1em;
    border-bottom: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
}
.newsImage {
    border: 1px solid #ccc;
}
.newsImageContainer {
    float:right;
    margin: 0 0 0.5em 1em;
    width: 202px;
}
.newsContent {
    padding: 0 1em 1em 1em;
}
.newsContent ul,
.newsContent li {
    display: block;
    list-style: none;
    list-style-image: none;
    margin: 0;
    padding: 0;
}
.newsAbout {
    display: block;
    color: &dtml-discreetColor;;
    font-size: &dtml-fontSmallSize;;
    padding: 0;
    margin-top: 0;
    list-style: none;
    list-style-image: none;
    float: right;
    text-align: right;
}
.newsAbout li {
    display: inline;
}
.newsFooter {

}
.newsFooter li {
    display: inline;
    margin: 0 1em 0 0;
}
.documentActions {
    margin: -2em 0.272em 0 0;
    padding: 0;
    text-align: right;
    float: right;
    background-color: transparent;
}
.documentActions ul {
    margin: 0;
    display: block;
    list-style-type: none;
    list-style-image: none;
    text-indent: 0;
}
.documentActions li {
    display: inline;
    margin: 0 0.1em;
    float: right;
}
.documentActions a {
    border: none !important;
    text-decoration: none;
    padding: 0em 0 0.2em 1.7em !important;
}

/* Status messages */
dl.portalMessage {
    background-color: white;
    border: 1px solid #74AE0B;
    margin:0;
    line-height: 1em;
}
dl.portalMessage a {
    color: black;
    border: none;
    text-decoration: underline;
}
dl.portalMessage dt,
dl.portalMessage dd {
    line-height: 1em;
    margin: 0;
    padding: 0.5em 1em 0.5em 2.5em;
}
dl.info {
    background: transparent url(&dtml-portal_url;/icon_status_erledigt.gif) no-repeat 0.3em 0.3em;
    border-color: &dtml-green;; 
    color: &dtml-green;;
}
dl.info dt {
    display: none;
}
dl.warning {
    /* XXX: warning class is not defined in the design fundamentals */
    background: transparent url(&dtml-portal_url;/icon_status_ueberfaellig.gif) no-repeat 0.3em 0.3em;
    border-color: #d80;
    color: #d80;
}
dl.warning dt {
    display: none;
}
dl.error {
    background: transparent url(&dtml-portal_url;/icon_status_ueberfaellig.gif) no-repeat 0.3em 0.3em;
    border-color: &dtml-red;;
    color: &dtml-red;;
}
dl.error dd:before {
    content: "- ";
}

.documentDescription {
    /* The summary text describing the document */
    font-weight: bold;
    display: block;
    margin: 0 0 1.091em 0;
    line-height: 1.455em;
    color: &dtml-grey5;;
}
div.documentByLine {
    background-color: &dtml-grey2;;
    font-weight: normal;
    color: &dtml-grey5;;
    padding: 0.25em 1em;
}
div.documentByLine a {
    color: &dtml-grey5;;
    text-decoration: none;
}
div.documentByLine a:hover {
    text-decoration: underline;
}
div.documentByLine span:first-child {
    border-left-style: none;
    padding-left: 0;
}
div.documentByLine span {
    border-left: 1px solid &dtml-grey5;;
    padding-left: 0.4em;
}
dl.searchResults span.documentByLine {
    display: block;
}
.discussion {
    margin-top: 1em;
}
.visualHighlight {
    background-color: #ffc;
}
.discreet {
    color: &dtml-discreetColor;;
    font-size: &dtml-fontSmallSize;;
    font-weight: normal;
}
.pullquote {
    padding: 0 1em 0 1em;
    margin: 0 0 1em 1em;
    font-weight: bold;
    color: #CC9933;
    float: right;
    width: 35%;
    clear: right;
    background-color: &dtml-backgroundColor;;
    border-left: 4px solid &dtml-globalBorderColor;;
}
.callout {
    font-weight: bold;
    color: #CC9933;
    padding: 0px 1em;
}

.notify,
.documentEditable * .notify {
    border: &dtml-borderWidth; &dtml-borderStyle; &dtml-notifyBorderColor;;
}
.card {
    background-color: &dtml-globalBackgroundColor;;
    border-color: &dtml-globalBorderColor;;
    border-width: &dtml-borderWidth;;
    border-style: &dtml-borderStyle;;
    float: left;
    margin: 1em;
    text-align: center;
    width: 110px;
    padding: 1em 0;
}
.card a {
    text-decoration: none;
}
.portrait {
    background-color: &dtml-globalBackgroundColor;;
    border-color: &dtml-globalBorderColor;;
    border-width: &dtml-borderWidth;;
    border-style: &dtml-borderStyle;;
    font-size: &dtml-fontSmallSize;;
    margin: 0.5em;
    padding: 1em 0 0.5em 0;
    text-align: center;
    width: 100px;
}
.portraitPhoto {
    border: 1px solid black;
}

/* default table style */
table.listing,
table.grid,
table.invisible,
table.vertical {
    border-collapse: collapse;
    margin-bottom: 1em;
}
table.listing th,
table.grid th,
table.invisible th,
table.vertical th {
    padding: 1.091em 0.454em 0.909em 0;
    text-align: left;
    vertical-align: top;
}
table.listing td,
table.grid td,
table.invisible td,
table.vertical td {
    padding: 0.272em 0 0.272em;
    vertical-align: top;
}
table a {
    text-decoration: none;
}

/* listing table style - default table with borders */
table.listing {
    background: transparent url(&dtml-portal_url;/table_top.gif) top left repeat-x;
}
table.listing td {
    border-left: 0.272em &dtml-backgroundColor; solid;
    border-bottom: 1px solid &dtml-zugblue;;
    border-top:  1px solid &dtml-zugblue;;
}
table.listing td:first-child,
table.listing th:first-child  {
    border-left-style: none !important;
}

/* Vertical addition class */
table.vertical {
    background-image: none;
}
table.vertical th {
    border-top: 1px solid &dtml-zugblue;;
    border-bottom: 1px solid &dtml-zugblue;;
    padding-top: 0.272em;
}

/* plain table class with light gray borders */
table.plain,
table.plain td,
table.plain th {
    border: 1px solid &dtml-grey1;;
    padding: 0.272em;
    border-collapse: collapse;
}

/* grid table - table with borders on all sides */
table.grid {
    border: 1px solid &dtml-zugblue;;
}
table.grid td,
table.grid th {
    border-style: solid solid solid solid;
    border-color: &dtml-zugblue;;
    border-width: 1px;
    padding: 0.272em;
}

/* invisible class - no border */
table.invisible,
table.invisible th,
table.invisible td {
    border-style: none;
}
table caption {
    text-align: left;
    font-weight: bold;
}

/* Batch selector */
.listingBar {
    text-align: center;
    vertical-align: top;
    margin: 1em 0em;
    clear: both;
    color: &dtml-grey5;;
    display: inline-block;
}
.listingBar a,
.listingBar>span {
    text-decoration: none;
    border-color: &dtml-grey5;;
    border-style: solid;
    border-width: 0.091em 0.182em 0.182em 0.091em;
    padding: 0 0.182em;
    margin: 0;
    color: &dtml-grey5;;
}
.listingBar span {
    display: none;
}
.listingBar span.gap {
    border-style:none;
    background: transparent none !important;
    display: inline;
}
.listingBar span.current {
    background: &dtml-zugblue; none !important;
    border-color: &dtml-zugblue; !important;
    color: white;
    display: inline;
}
.listingBar span.previous,
.listingPrevious {
    display: block;
    text-align: left;
    float: left;
    border-style: none;
    margin-right: 0;
}
.listingBar span.next,
.listingNext {
    display: block;
    text-align: right;
    float: right;
    border-style: none;
    margin-right: 0;
}


/* tile items */
.tileItem {
    border-top: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
    padding-top: 0.5em;
    margin-top: 0.5em;
}
.tileHeadline {
    border: none;
    font-size: 110%;
    font-weight: bold;
}
.tileHeadline a {
    text-decoration: none;
}
.tileBody {
    margin-bottom: 0.5em;
}
.tileImage {
    float: right;
    margin: 0 0 0.5em 0.5em;
    border: 1px solid black;
}
.eventDetails {
    float: right;
    clear: right;
    margin: 0 0 0.5em 1em;
}

/* Useful deviations from regular style on elements */

/* List classes without markers */
ul.visualNoMarker,
ol.visualNoMarker {
    list-style-type: none;
    list-style-image: none;
    margin: 0.5em 0 0 0;
    line-height: 1em;
}

ul.discreet {
    list-style-image: none;
    list-style-type: disc;
}

textarea.proportional {
    font: 100% <dtml-var fontFamily>;
}
.productCredits {
    text-align: right;
    font-size: &dtml-fontSmallSize;;
    clear: both;
    font-weight: normal;
    color: &dtml-discreetColor;;
}
#portal-footer {
    background-color: &dtml-globalBackgroundColor;;
    border-top: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
    border-bottom: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
    float: none;
    line-height: 1.2em;
    margin: 2em 0em 1em 0em;
    padding: 0.5em 0em 1em 0em;
    text-align: center;
}
#portal-footer p {
    margin: 0.25em 0;
}
#portal-footer a {
    text-decoration: none;
    color: &dtml-linkColor;;
    border: none;
}
#portal-footer a:visited {
    color: &dtml-linkColor;;
}
#portal-footer a:hover {
    text-decoration: underline;
}
#portal-colophon {
    float: none;
    margin: 0 0 1em 0;
    padding: 0 0 1em 0;
    text-align: center;
    color: #666;
}
#portal-colophon ul {
    list-style-image: none;
    list-style-type: none;
}
#portal-colophon ul li {
    display: inline !important;
    font-size: &dtml-fontSmallSize;;
