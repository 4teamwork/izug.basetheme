}

#calnext {
	background-image:url(&dtml-portal_url;/datepicker_next.gif) !important;
}

/* wizard */
.wizard {

}
.wizard .wizardSteps {
    margin: 1em 0;
    display: block;
    overflow: hidden;
    border-left: 1px solid #111111;
    background: transparent url(&dtml-portal_url;/wizard_bg.gif) repeat-x top left;
    padding-right: 3em;
    border-right: 0.1em solid #bbbbbb;
}
.wizard .wizardSteps li {
    display: block;
    float: left;
    background: url(&dtml-portal_url;/wizard_inactive.gif) no-repeat top right;
    height: 2em;
    margin: 0 0 0 0;
    text-indent: 0;
}
.wizard .wizardSteps li a {
    line-height: 1.8181em;
    list-style: inline;
    border-style: solid none;
    border-width: 0.1em;
    border-color: #bbbbbb;
    margin-right: 1em;
    padding-left: 1.545em;
    padding-right: 2em;
    margin-left: -0.909em;
    display: block;
    color: #bbbbbb;
}
.wizard .wizardSteps li.selected,
.wizard .wizardSteps li.visited  {
    background: url(&dtml-portal_url;/wizard_active.gif) no-repeat top right;
}
.wizard .wizardSteps li.selected a,
.wizard .wizardSteps li.visited a {
    border-color: #111111;
    color: #111111;
}
.wizard .wizardSteps li.selected a {
    font-weight: bold;
}
.wizard .wizardContent {
    display: block;
    clear: both;
}
#debug_info {
    clear: both;
    position: absolute;
    top: 10.7em;
}
.collapsibleLegend {
    float: left;
    width: 1em;
    height: 1.5em;
    margin-right: 1em;
    background: url(&dtml-portal_url;/icon_open.gif) no-repeat scroll 50% 50% transparent;
    cursor: pointer;
}
.collapsibleLegend.closed {
    background: url(&dtml-portal_url;/icon_closed.gif) no-repeat scroll 50% 50% transparent;
}
